<h1 class="page-header">Events for machine <%= @machine_name %></h1>
<%= link_to new_event_path, :class => 'clearfix' do %>
	<button class="btn btn-large btn-primary" type="button">Create new event</button>
<% end %>
<p></p>
<div class="tabbable">
	<ul class="nav nav-tabs">
		<li class="active"><a href="#tab1" data-toggle="tab">All events</a></li>
		<li><a href="#tab2" data-toggle="tab">My events</a></li>
	</ul>
	<div class="tab-content">
		<div class="tab-pane active" id="tab1">
			<%= render(:partial => "table", :locals => { :event_listing => @machine_events_all, :row_number => @row_number_tab1 }) %>
		</div>
		<div class="tab-pane" id="tab2">
			<%= render(:partial => "table", :locals => { :event_listing => @machine_user_events, :row_number => @row_number_tab2 }) %>
		</div>
	</div>
</div>