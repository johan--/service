<h1>Listing machine_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>Manufacturer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @machine_groups.each do |machine_group| %>
  <tr>
    <td><%= machine_group.name %></td>
    <td><%= machine_group.type %></td>
    <td><%= machine_group.manufacturer_id %></td>
    <td><%= link_to 'Show', machine_group %></td>
    <td><%= link_to 'Edit', edit_machine_group_path(machine_group) %></td>
    <td><%= link_to 'Destroy', machine_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

